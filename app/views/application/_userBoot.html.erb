<div class="row">
  <div class="large-12 columns">
    <h3><%= @user.username %>'s Items</h3><hr>
  </div>

  <% @user.items.each do |item| %>
    <div class="large-3 small-6 columns">
      <%=  link_to (image_tag item.image, class:"image-sizer"), item_path(item) %>
      <div class="panel mobile-tile-item">
        <p><%= link_to item.name, item_path(item) %></p>
      </div>
    </div>
  <% end %>
</div>
